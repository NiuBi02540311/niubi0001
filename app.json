{
  "pages": [
    "pages/home/home",
    "pages/message/message",
    "pages/market/market",
    "pages/contract/contract",
    "pages/goodlist/goodlist",
    "pages/goodadd/goodadd",
    "pages/goodedit/goodedit",
    "pages/goodshow/goodshow",
    "pages/customer/customer",
    "pages/index/index",
    "pages/test/test",
    "pages/logs/logs"
  
  ],
  "permission": {
    "scope.userLocation": {
      "desc": "你的位置信息将用于小程序位置接口的效果展示" 
    }
  },
  "requiredPrivateInfos": [ 
    "getLocation",
    "onLocationChange",
    "startLocationUpdateBackground",
    "chooseAddress"
  ],
  "preloadRule": {
    "pages/contract/contract":{ "packages": ["p1"],"network": "all" },
    "pages/message/message":{ "packages": ["p2"],"network": "wifi" }
  },
  "subPackages": [
    {
      "root": "pkgA",
      "name": "p1",
      "pages": [
        "pages/cat/cat",
        "pages/dog/dog"
      ]
    },
    {
      "root": "pkgB",
      "name": "p2",
      "pages": [
        "pages/apple/apple"
      ]
    }
  ],
  "tabBar": {
    "custom": true,
    "color": "#000000",
    "selectedColor": "#FE9132",
    "list": [
      {
      "pagePath": "pages/home/home",
      "text": "主页",
      "iconPath": "/images/tabs/home2.png",
      "selectedIconPath": "/images/tabs/home1.png"
    },
    {
      "pagePath": "pages/message/message",
      "text": "消息",
      "iconPath": "/images/tabs/message1.png",
      "selectedIconPath": "/images/tabs/message2.png"
    },
    {
      "pagePath": "pages/market/market",
      "text": "圈子",
      "iconPath": "/images/tabs/ren2.png",
      "selectedIconPath": "/images/tabs/ren1.png"
    },
    {
      "pagePath": "pages/customer/customer",
      "text": "我的"
    }
  ]
  },
  "window": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "家中物品管理",
    "navigationBarBackgroundColor": "#ffffff"
  },
  "componentFramework": "glass-easel",
  "sitemapLocation": "sitemap.json",
  "lazyCodeLoading": "requiredComponents",
  "__usePrivacyCheck__": false,
  "usingComponents": {
    "van-button": "@vant/weapp/button/index",
    "van-toast": "@vant/weapp/toast/index",
    "van-tabbar": "@vant/weapp/tabbar/index",
    "van-tabbar-item": "@vant/weapp/tabbar-item/index",
    "van-grid": "@vant/weapp/grid/index",
    "van-grid-item": "@vant/weapp/grid-item/index",
    "van-card": "@vant/weapp/card/index",
    "van-nav-bar": "@vant/weapp/nav-bar/index",
    "van-tag": "@vant/weapp/tag/index",
    "van-notify": "@vant/weapp/notify/index",
    "van-field": "@vant/weapp/field/index",
    "van-calendar": "@vant/weapp/calendar/index",
    "van-dialog": "@vant/weapp/dialog/index",
    "van-search": "@vant/weapp/search/index",
    "banner": "/components/Banner/Banner",
    "van-switch": "@vant/weapp/switch/index",
    "van-cell": "@vant/weapp/cell/index",
    "van-cell-group": "@vant/weapp/cell-group/index",
    "van-row": "@vant/weapp/row/index",
    "van-col": "@vant/weapp/col/index",
    "van-image": "@vant/weapp/image/index",
    "van-action-sheet": "@vant/weapp/action-sheet/index",
    "van-popup": "@vant/weapp/popup/index",
    "van-collapse": "@vant/weapp/collapse/index",
    "van-collapse-item": "@vant/weapp/collapse-item/index",
    "fagui": "/components/fagui/fagui"
  }
}